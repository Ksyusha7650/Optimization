<Window x:Class="CourseWorkOptimization.ChartsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CourseWorkOptimization"
        xmlns:wpf="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="ChartsWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen" Topmost="True">
    <Window.DataContext>
        <local:MyChart/>
    </Window.DataContext>
    <Grid>
        <wpf:PlotView Model="{Binding MyModel}"/>
    </Grid>

</Window>
