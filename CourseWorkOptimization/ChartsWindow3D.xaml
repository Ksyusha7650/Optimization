<Window x:Class="CourseWorkOptimization.ChartsWindow3D"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:CourseWorkOptimization"
          xmlns:chartDirector="clr-namespace:ChartDirector;assembly=ChartDirector.Net.Desktop.Controls"
        mc:Ignorable="d"
        Title="3D график" Height="450" Width="500" WindowStartupLocation="CenterScreen" Topmost="True">
    <Grid HorizontalAlignment="Left" Width="505">
        <chartDirector:WPFChartViewer x:Name="Chart3D" Margin="0,10,0,0" ChartSizeMode="StretchImage" Width="400" Height="400" VerticalAlignment="Top" HorizontalAlignment="Center"/>
    </Grid>

</Window>
